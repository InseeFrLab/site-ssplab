jQuery(function($){
    $(document).ready(initMegaMenu);
});
    
function initMegaMenu() {
  var megaMenu = document.getElementById('mega-menu');
  // keyboard stuff
  
  
  jQuery(function ($) {
	$("#mega-menu").on('keydown', 'li a', function (event) {
		var elt = $(this);
		var target = event.target;
		var which = event.which;
		
		if (which === 39) { // RIGHT
		  if (isTopLevel(elt)) {
		    // top level item
		    var nextTopItem = adjacentTopLevelItem($, target, 'next');
		
		    if (nextTopItem) {
		      event.preventDefault();
		      nextTopItem.focus();
		    }
		  } else {
		    // dropdown item
		    var nextDropletItem = adjacentDropdownItem($, target, 'next');
		    if (nextDropletItem) {
		      event.preventDefault();
		      nextDropletItem.focus();
		    }
		  }
		} else if (which === 37) { // LEFT
		  if (isTopLevel(elt)) {
		    // top level item
		    var prevTopItem = adjacentTopLevelItem($, target, 'prev');
		
		    if (prevTopItem) {
		      event.preventDefault();
		      prevTopItem.focus();
		    }
		  } else {
		    // dropdown item
		    var prevDropItem = adjacentDropdownItem($, target, 'prev');
		    if (prevDropItem) {
		      event.preventDefault();
		      prevDropItem.focus();
		    }
		  }
		} else if (which === 40) { // DOWN
		  if (isTopLevel(elt) && hasDropdown($, target)) {
		    // top level item w/ dropdown -- open dropdown
		    openDropdown($, target);
		  } else {
		    // dropdown item
		    var nextDropItem = adjacentDropdownItem($, target, 'next');
		
		    if (nextDropItem) {
		      event.preventDefault();
		      nextDropItem.focus();
		    }
		  }
		} else if (which === 38) { // UP
		  if (!isTopLevel(elt)) {
		    if (isFirstDropItem($, target)) {
		      event.preventDefault();
		      var top = closeDropdown($, target);
		      setTimeout(function () {
		        top.focus();
		      }, 0);
		    } else {
		      var prevDropAnchor = adjacentDropdownItem($, target, 'prev');
		
		      if (prevDropAnchor) {
		        event.preventDefault();
		        prevDropAnchor.focus();
		      }
		    }
		  }
		} else if (which === 27) { // ESCAPE
		  if (!isTopLevel(elt)) {
		    var topper = closeDropdown($, target);
		    setTimeout(function () {
		      topper.focus();
		    }, 0);
		  }
		} else if (which === 9 && event.shiftKey) { // SHIFT + TAB
		  if (!isTopLevel(elt) && isFirstDropItem($, target)) {
		    event.preventDefault();
		    var topA = closeDropdown($, target);
		    setTimeout(function () {
		      topA.focus();
		    }, 0);
		  }
		} else if (which === 9) { // TAB
		  if (!isTopLevel(elt) && isLastDropItem($, target)) {
		    event.preventDefault();
		    var topItem = closeDropdown($, target);
		    var nextLi = $(topItem.parentNode).next()[0];
		    var nextAnchor = $('a', nextLi)[0];
		    nextAnchor.focus();
		  }
		} else if (which === 13 || which === 32) {
		  if (isTopLevel(elt) && $(target.parentNode).hasClass('has-drop')) {
		    openDropdown($, target);
		  }
		  if (!isTopLevel(elt) && which === 32){
		  	var topper = closeDropdown($, target);
		    setTimeout(function () {
		      topper.focus();
		    }, 0);
		  }
		}
	})
  
  });

  // mouse stuff
  /*
  $(' > li.has-drop', $("#mega-menu")).each(function () {
    var dropdown = $('.dropdown', $(this)[0]);

    $(this).hover(
      function () {
        $(this).addClass('active');
        $('.dropdown', megaMenu).each(function (index, drop) {
          if (drop !== dropdown) {
            $(drop).hide();
          }
        });

        $(dropdown).slideDown(100);

      }, function () {
        $(this).removeClass('active');
        $(dropdown).slideUp(100);
      }
    );
  });
  */
}



// determines if the item is a top-level one
function isTopLevel(item) {
	return item.is('#mega-menu > li > a');
}

// determines if the item has a dropdown
function hasDropdown($, item) {
  return $(item.parentNode).hasClass('has-drop');
}

// determines if the item is the first in the dropdown
function isFirstDropItem($, item) {
  var drop = $(item).closest('.dropdown')[0];

  var firstInDrop =  $(drop).find('[role=menuitem]')[1];
  //var firstInDrop = $(subElt).children("a").first();

  return $(firstInDrop)[0] == $(item)[0];

}

// determines if the item is the last in the dropdown
function isLastDropItem($, item) {
  var drop = $(item).closest('.dropdown')[0];
  var lastInDrop = $('li a', drop).last()[0];

  return lastInDrop === item;
}

// finds the adjacent top level item
function adjacentTopLevelItem($, item, dir) {
  var li = item.parentNode; // <li />

  var adjacentLi = (dir === 'next') ?
                    $(li).next()[0] :
                    $(li).prev()[0];
  var adjacentItem = $(adjacentLi).children("a");//adjacentLi && $('a', adjacentLi)[0];

  return adjacentItem;
}

// finds the next or prev dropdown item
function adjacentDropdownItem($, item, dir) {
  var adjacentDropItem;
  var li = item.parentNode;
  var adjacentSameCol = (dir === 'next') ?
                        $(li).next()[0] :
                        $(li).prev()[0];
  if (adjacentSameCol && !($(adjacentSameCol).children("a").attr('role') == 'menuitem')){
  	adjacentSameCol = (dir === 'next') ?
                        $(li).next().next()[0] :
                        $(li).prev().prev()[0];
  }
  if (adjacentSameCol && $(adjacentSameCol).children("a").attr('role') == 'menuitem') {
    // there is one in the same col
    adjacentDropItem = $('a', adjacentSameCol)[0];
  } else {
    // lets look for one in the adjacent column
    var col = $(li).closest('.column-holder')[0];
 
    var adjacentCol = (dir === 'next') ?
                      $(col).next()[0] :
                      $(col).prev()[0];
    if (adjacentCol) {
      // we've found the adjacent column
      var adjacentItem = (dir === 'next') ?
                          $(adjacentCol).find('[role=menuitem]').first() :
                          $(adjacentCol).find('[role=menuitem]').last();

      if (adjacentItem) {
        adjacentDropItem = adjacentItem;
      }
    }
  }

  return adjacentDropItem;
}


function openDropdown($, item) {
  //$(item.parentNode).addClass('active');
  //var droplet = $(item).next()[0];
  // open the dropdown...
  //$(droplet).slideDown(100);
  // ...and focus the first item
  setTimeout(function () {
  	var subElt =  $(item).parent().find('[role=menuitem]')[1];
  	$(subElt).focus();
  }, 100);
}

function closeDropdown($, item) {

 /* var droplet = $(item).closest('.dropdown')[0];
  var topLevelItem = $(droplet).prev()[0];
  $(topLevelItem.parentNode).removeClass('active');
  $(droplet).slideUp(100);
  */
  var drop = $(item).closest('.dropdown')[0];
  $(drop).removeClass('open');
  var topLevelItem = $(drop).children("a").first();

  return topLevelItem;
}


